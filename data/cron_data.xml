<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <record id="ir_cron_replica_partner_report" model="ir.cron">
      <field name="name">Replica: daily heavy partner report</field>
      <field name="model_id" ref="model_replica_report_runner"/>
      <field name="state">code</field>
      <field name="code">model.run_heavy_partner_report()</field>
      <field name="active" eval="False"/> <!-- disabled by default -->
      <field name="interval_type">days</field>
      <field name="interval_number">1</field>
      <field name="numbercall">-1</field>
      <field name="doall" eval="False"/>
      <field name="priority" eval="10"/>
    </record>
  </data>
</odoo>
